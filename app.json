{
  "expo": {
    "name": "ClubSeason",
    "slug": "club-season",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ClubSeason.png",
    "scheme": "club-season",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ClubSeason.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ClubSeason",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload golf course photos and player avatars.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for golf courses and player profiles.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ClubSeason.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ClubSeason.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e86e1703-6d6c-49ca-b5f0-c7942ca7dafc"
      }
    }
  }
}
